function rollDice() {
    console.log(`first random number ${Math.random()}`);
    console.log((Math.random() * 6) );
    console.log(Math.floor(Math.random() * 6) + 1);
    
    
    
  return Math.floor(Math.random() * 6) + 1;
}

const diceRoll = rollDice();
console.log(`You rolled a ${diceRoll}`);